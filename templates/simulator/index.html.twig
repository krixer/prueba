{% extends 'base.html.twig' %}

{% block title %}Hello SimulatorController!{% endblock %}

{% block content %}
    <table class="table table-striped table-bordered table-hover table-dark">
        <thead>
            <tr>
                {% for header in headers %}
                    <th>{{ header }}</th>
                {% endfor %}
            </tr>
        </thead>
        <tbody>
	        {% for cell in rows %}
	            <tr>
	                {% for value in cell %}
	                    <td>{{ value }}</td>
	                {% endfor %}
	            </tr>
	        {% endfor %}
        </tbody>
    </table>
{% endblock %}